<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Wild Series{% endblock %}</title> {# Default title #}
        <link rel="icon" type="image/x-icon" href="{{ asset('build/images/favicon.ico') }}" />
        
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </head>
    <body class="bg-secondary">
        <header>
            {{include('_navbar.html.twig')}}
        </header>
    
        <main class="p-2">
            {% block main %}{% endblock %}
        </main>
    </body>
</html>
